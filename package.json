{
  "license": "MIT",
  "name": "web-starter",
  "repository": "https://github.com/seonghyeonkimm/web-starter.git",
  "scripts": {
    "postinstall": "yarn codegen && yarn prisma:generate && yarn run relay",
    "dev": "NODE_ENV=development concurrently 'next dev' 'yarn run relay --watch' 'docker-compose up -d'",
    "test": "NODE_ENV=development concurrently 'yarn run relay --watch' 'dotenv -e .env.local -- jest --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:cli": "dotenv -e .env.local -- prisma",
    "prisma:generate": "yarn run prisma:cli generate"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.5",
    "@graphql-yoga/node": "^2.13.13",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.5.0",
    "@vanilla-extract/recipes": "^0.2.5",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "graphql": "^16.6.0",
    "graphql-modules": "^2.1.0",
    "next": "^13.0.0",
    "next-auth": "^4.15.0",
    "next-themes": "^0.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "relay-nextjs": "^0.10.1",
    "relay-runtime": "^14.1.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.13.8",
    "@graphql-codegen/graphql-modules-preset": "^2.5.4",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-codegen/typescript-resolvers": "^2.7.5",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-relay": "^14.1.2",
    "@types/relay-runtime": "^14.1.4",
    "@types/relay-test-utils": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/jest-transform": "^1.0.1",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "concurrently": "^7.5.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "prisma": "4.5.0",
    "relay-compiler": "^14.1.0",
    "relay-test-utils": "^14.1.0",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
